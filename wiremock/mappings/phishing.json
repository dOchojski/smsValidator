{
  "priority": 1,
  "request": {
    "method": "POST",
    "urlPath": "/v1eap1:evaluateUri",
    "bodyPatterns": [
      { "matchesJsonPath": "$[?(@.uri =~ /.*bank.*|.*verify.*/i)]" }
    ]
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "scores": [
        { "threatType": "SOCIAL_ENGINEERING", "confidenceLevel": "HIGHER" }
      ]
    },
    "headers": {
      "Content-Type": "application/json"
    }
  }
}
